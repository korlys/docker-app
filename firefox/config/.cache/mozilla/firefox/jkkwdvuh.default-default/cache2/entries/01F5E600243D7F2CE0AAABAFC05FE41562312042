            js_pop_area_user = $('#js-pop-area-user'), //Áî®Êà∑ÂºπÁ™óÂå∫Âüü
            js_pop_user = $('.js-pop-user'), //Áî®Êà∑Áõ∏ÂÖ≥ÂºπÁ™ó
            js_pop_close = $('.js-pop-close'), //ÂÖ≥Èó≠ÂºπÁ™ó
            js_register_now = $('#js-register-now'), //Á´ãÂç≥Ê≥®ÂÜå
            js_return_login = $('#js-return-login'), //ËøîÂõûÁôªÂΩï
            js_forgot_pwd = $('#js-forgot-pwd'), //ÂøòËÆ∞ÂØÜÁ†Å
            js_error_tip = $('.js-error-tip'), //ÈîôËØØÊèêÁ§∫
            js_input_txt = $('.js-input-txt'), //ËæìÂÖ•Ê°ÜÈõÜÂêà
            local_ip_address = '' //Êú¨Âú∞ipÂú∞ÂùÄ
    }
    // ÁôªÂΩïÁõ∏ÂÖ≥ÂèòÈáè
    {
        var js_login_user_name = $('#js-login-user-name'), //ÁôªÂΩïÁî®Êà∑Âêç
            js_login_user_pwd = $('#js-login-user-pwd'), //ÁôªÂΩïÂØÜÁ†Å
            js_btn_login = $('#js-btn-login'), //Áî®Êà∑ÁôªÂΩïÊåâÈíÆ
