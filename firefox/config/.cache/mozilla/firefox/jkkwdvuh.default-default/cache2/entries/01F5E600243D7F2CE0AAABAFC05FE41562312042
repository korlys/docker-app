            js_register_user_phone = $('#js-register-user-phone'), //Ê≥®ÂÜåÊâãÊú∫Âè∑Á†Å
            js_register_user_pwd = $('#js-register-user-pwd'), //Ê≥®ÂÜåÂØÜÁ†Å
            bar = $('.pop-register .form-item-range .rangebox .bar'), // ÊªëÂùó
            redbg = $('.pop-register .form-item-range .rangebox .redbg'), // ÊªëÂùóÁ∫¢Ëâ≤ËÉåÊôØ
            js_register_sms_code = $('#js-register-sms-code'), //Ê≥®ÂÜåÁü≠‰ø°È™åËØÅÁ†ÅËæìÂÖ•Ê°Ü
            js_register_get_sms_code = $('#js-register-get-sms-code'), //Ëé∑ÂèñÁü≠‰ø°È™åËØÅÁ†Å
            js_check_protocol = $('#js-check-protocol'), //ÊòØÂê¶ÂêåÊÑèÊ≥®ÂÜåÂçèËÆÆ
            js_btn_register = $('#js-btn-register'), //Áî®Êà∑Ê≥®ÂÜåÊåâÈíÆ
            js_register_input_txt = $('.js-register-input-txt'), //Áî®Êà∑Ê≥®ÂÜåËæìÂÖ•Ê°ÜÈõÜÂêà
            js_register_error_tip = $('#js-register-error-tip') //Ê≥®ÂÜåÈîôËØØÊèêÁ§∫
    }
    //ÂøòËÆ∞ÂØÜÁ†ÅÁõ∏ÂÖ≥ÂèòÈáè
    {
        var js_forgot_user_phone = $('#js-forgot-user-phone'), //ÂøòËÆ∞ÂØÜÁ†Å-ÊâãÊú∫Âè∑Á†Å
            js_forgot_user_pwd = $('#js-forgot-user-pwd'), //ÂøòËÆ∞ÂØÜÁ†Å-ÂØÜÁ†Å
            js_forgot_repeat_pwd = $('#js-forgot-repeat-pwd'), //ÂøòËÆ∞ÂØÜÁ†Å-Á°ÆËÆ§ÂØÜÁ†Å
            js_forgot_img_code = $('#js-forgot-img-code'), //ÂøòËÆ∞ÂØÜÁ†Å-ÂõæÁâáÈ™åËØÅÁ†ÅËæìÂÖ•Ê°Ü
            js_forgot_captcha_code = $('#js-forgot-captcha-code'), //ÂøòËÆ∞ÂØÜÁ†Å-ÂõæÁâáÈ™åËØÅÁ†Å
            js_forgot_sms_code = $('#js-forgot-sms-code'), //ÂøòËÆ∞ÂØÜÁ†Å-Áü≠‰ø°È™åËØÅÁ†ÅËæìÂÖ•Ê°Ü
            js_forgot_get_sms_code = $('#js-forgot-get-sms-code'), //ÂøòËÆ∞ÂØÜÁ†Å-Ëé∑ÂèñÁü≠‰ø°È™åËØÅÁ†Å
            js_btn_forgot = $('#js-btn-forgot'), //ÂøòËÆ∞ÂØÜÁ†Å-Áî®Êà∑Á°ÆËÆ§ÊåâÈíÆ
            js_forgot_input_txt = $('.js-forgot-input-txt'), //ÂøòËÆ∞ÂØÜÁ†Å-Áî®Êà∑ËæìÂÖ•Ê°ÜÈõÜÂêà
            js_forgot_error_tip = $('#js-forgot-error-tip'), //ÂøòËÆ∞ÂØÜÁ†Å-ÈîôËØØÊèêÁ§∫
            forgot_user_phone_is_exist = false, //ÂøòËÆ∞ÂØÜÁ†Å-ÊâãÊú∫Âè∑Á†ÅÊòØÂê¶Â≠òÂú®
            forgot_user_phone_is_blur = false //ÂøòËÆ∞ÂØÜÁ†Å-ÊâãÊú∫Âè∑Á†ÅËæìÂÖ•Ê°ÜÊòØÂê¶Â∑≤ÁªèÂ§±ÂéªÁÑ¶ÁÇπ
    }
    // Â∏∏ËßÑÊìç‰Ωú
    {
        /**
         *
         * ÊòæÁ§∫Âú®Êüê‰∏™ÂÖÉÁ¥†ÈõÜÂêàÂÜÖÊåáÂÆöÁ¥¢ÂºïÁöÑÂÖÉÁ¥†ÔºåÈÄöÂ∏∏Áî®‰∫éÊâìÂºÄÂºπÁ™ó
         * ÂèÇÊï∞1ÔºöÂÖÉÁ¥†ÈÄâÊã©Âô®ÔºåÁ±ªÂûãÔºöString || Object
         * ÂèÇÊï∞2ÔºöÂÖÉÁ¥†Á¥¢ÂºïÔºåÁ±ªÂûãÔºöNumber || String
         */
        function openPopBox(ele, index) {
            $(ele).addClass('hide').eq(index).removeClass('hide')
        }

        // ÈöêËóèÊüê‰∏™ÂÖÉÁ¥†ÈõÜÂêàÔºåÈÄöÂ∏∏Áî®‰∫éÂÖ≥Èó≠ÊâÄÊúâÂºπÁ™ó
        function closePopBox(ele) {
            $(ele).addClass('hide')
        }

        // ÁÇπÂáªÂºπÂá∫ÁôªÂΩï
        js_pop_login_panel.on({
            click: function () {
                js_pop_area_user.removeClass('hide')
                openPopBox(js_pop_user, 0)
            }
        })

        // ÁÇπÂáªÂºπÂá∫Ê≥®ÂÜå
        js_pop_register_panel.on({
            click: function () {
                js_pop_area_user.removeClass('hide')
                openPopBox(js_pop_user, 1)
            }
        })

        // ÁÇπÂáªÁ´ãÂç≥Ê≥®ÂÜå
        js_register_now.on({
            click: function () {
                openPopBox(js_pop_user, 1)
            }
        })

        //ÁÇπÂáªËøîÂõûÁôªÂΩï
        js_return_login.on({
            click: function () {
                openPopBox(js_pop_user, 0)
            }
        })

        // ÁÇπÂáªÂøòËÆ∞ÂØÜÁ†Å
        js_forgot_pwd.on({
            click: function () {
                openPopBox(js_pop_user, 2)
            }
        })

        // ÂÖ≥Èó≠ÂºπÁ™ó
        js_pop_close.on({
            click: function () {
                var pop_area = $(this).closest('.js-pop-area')
                closePopBox(pop_area)
            }
        })
    }

    /**
     * ‰∏éË°®ÂçïÊ†°È™åÁõ∏ÂÖ≥Êìç‰Ωú
     */
    {
        // Ê∑ªÂä†ÊåáÂÆöÂÖÉÁ¥†ÁöÑclass
        function addAppointClass(ele, className, index) {
            ele.removeClass(className).eq(index).addClass(className)
        }

        // Áî®Êà∑ÂêçÂêàÊ≥ïÊÄßÊ†°È™å
        function checkUserNameLegal(str) {
            var re = /^[a-zA-Z\d]{6,20}$/
            return re.test(str)
        }

        // ÂØÜÁ†ÅÂêàÊ≥ïÊÄßÊ†°È™å
        function checkUserPwdLegal(str) {
            var re = /^[^]{6,20}$/
            return re.test(str)
        }

        // ÂéªÈô§Â≠óÁ¨¶‰∏≤È¶ñÂ∞æ‰∏§Á´ØÁ©∫Ê†º
        function trim(str) {
            var reg = /^\s+|\s+$/g
            return str.replace(reg, '')
        }

        // ÊâãÊú∫Âè∑Á†ÅÊ†°È™å
        function checkPhoneNum(phoneNum) {
            var reg = /^1\d{10}$/
            return reg.test(phoneNum)
        }

        // Ê†°È™åÊòØÂê¶‰∏∫Á∫ØÊï∞Â≠óÂ≠óÁ¨¶‰∏≤
        function checkPureDigits(str) {
            var reg = /\D/
            return reg.test(str)
        }

        // Ëé∑ÂèñÊú¨Âú∞ipÂú∞ÂùÄ
        function getLocalIPAddress() {
            $.ajax({
                url: 'https://api.ipify.org/?format=json',
                dataType: 'json',
                type: 'get',
                success: function (data) {
                    local_ip_address = data.ip
                }
            })
        }

        getLocalIPAddress()
    }
    /**
     * ‰∏éÁôªÂΩïÁõ∏ÂÖ≥ÁöÑÊìç‰Ωú
     */
    {
        // Ê∏ÖÈô§ÁôªÂΩïÈîôËØØÊèêÁ§∫
        function clearLoginErrorTip() {
            js_login_error_tip.addClass('vh')
        }

        // ÊòæÁ§∫ÁôªÂΩïÈîôËØØÊèêÁ§∫‰ø°ÊÅØ
        function showLoginErrorMsg(msg) {
            js_login_error_tip.html(msg).removeClass('vh')
        }

        // Áî®Êà∑ÁôªÂΩïÊ†°È™å
        function checkUserLogin() {
            var val1 = js_login_user_name.val(),
                val2 = js_login_user_pwd.val(),
                isValid = true
            val1 = trim(val1)
            val2 = trim(val2)
            js_login_user_name.val(val1)
            js_login_user_pwd.val(val2)
            if (val1.length === 0) {
                showLoginErrorMsg('ËØ∑ËæìÂÖ•Áî®Êà∑Âêç')
                isValid = false
            } else if (!checkUserNameLegal(val1)) {
                showLoginErrorMsg('Áî®Êà∑Âêç‰∏çÂêàÊ≥ï')
                isValid = false
            } else if (val2.length === 0) {
                showLoginErrorMsg('ËØ∑ËæìÂÖ•ÂØÜÁ†Å')
                isValid = false
            } else if (!checkUserPwdLegal(val2)) {
                showLoginErrorMsg('ÂØÜÁ†Å‰∏çÂêàÊ≥ï')
                isValid = false
            }
            return isValid
        }

        // ÁôªÂΩïÊó∂Âú®ÊØè‰∏™Ê†°È™åË°®ÂçïÂÖÉÁ¥†‰∏äÁõëÂê¨‰∫ã‰ª∂
        js_login_input_txt.on({
            keyup: function (e) {
                if (e.keyCode === 13) {
                    js_btn_login.trigger('click')
                }
            },
            blur: function () {
                $(this).closest('.form-item').removeClass('focus')
                //clearLoginErrorTip();
            },
            focus: function () {
                $(this).closest('.form-item').addClass('focus')
            }
        })

        // ÁÇπÂáªÁ´ãÂç≥ÁôªÂΩï
        js_btn_login.on({
            click: function (e) {
                e.stopPropagation()
                if (checkUserLogin() && !this.canCommit) {
                    var that = this
                    this.canCommit = true
                    // ajaxÊèê‰∫§Ë°®Âçï
                    $.ajax({
                        type: 'post',
                        url: '/api/login',
                        data: {
                            name: js_login_user_name.val(),
                            password: js_login_user_pwd.val()
                        },
                        success: function (data) {
                            that.canCommit = false
                            if (data.status == 200) {
                                window.location.reload()
                            } else {
                                showLoginErrorMsg(data.msg)
                            }
                        }
                    })
                }
            }
        })
    }

    /**
     * ‰∏éÊ≥®ÂÜåÁõ∏ÂÖ≥ÁöÑÊìç‰Ωú
     */
    {
        const max = 329
        const $body = $('body')
        const bartextsvg = bar.find('svg>use')
        // ÊªëÂùó‰∫ã‰ª∂
        let barmove = null
        let barIsOk = false // ÊªëÂùóÊòØÂê¶ÂÆåÊàê
        let msgIsOk = false  // Áü≠‰ø°È™åËØÅÁ†ÅÊòØÂê¶ÊàêÂäüÂèëÈÄÅ
        bar.on('mousedown', e => {
            const { left: parX } = bar.parent().offset()
            const x1 = e.offsetX + parX
            if (!barIsOk) {
                bar.css('transition', 'none')
                redbg.css('transition', 'none')
                $body.on(
                    'mousemove',
                    (barmove = e => {
                        let x = e.pageX - x1
                        if (x < 0) x = 0
                        if (x >= max) {
                            x = max
                            barIsOk = true
                            bartextsvg.attr('xlink:href', '#icon-dagou')
                            bar.css({
                                'background-color': 'green',
                                color: 'white'
                            })
                            bar.siblings('p').text('ÊªëÂùóÈ™åËØÅÂ∑≤ÂÆåÊàê')
                        }
                        bar.css('left', x)
                        redbg.width(x)
                    })
                )
            }
        })
        $body.mouseup(() => {
            $body.off('mousemove', barmove)
            if (!barIsOk) {
                bar.css({
                    left: 0,
                    transition: 'all 0.3s'
                })
                redbg.css({
                    transition: 'all 0.3s',
                    width: 0
                })
            }
        })

        // ÂØÜÁ†ÅÊ°ÜÂàáÊç¢ÊòæÁ§∫
        let isopen = false
        js_register_user_pwd
            .siblings('svg:last')
            .click(function () {
                isopen = !isopen
                let text = '#icon-biyan'
                let type = 'password'
                if (isopen) {
                    text = '#icon-icon-eye-open'
                    type = 'text'
                }
                $(this).find('use').attr('xlink:href', text)
                js_register_user_pwd.attr('type', type)
            })

        // Ê∏ÖÈô§Ê≥®ÂÜåÈîôËØØÊèêÁ§∫
        function clearRegisterErrorTip() {
            js_register_error_tip.addClass('vh')
        }


        // ÊòæÁ§∫Ê≥®ÂÜåÈîôËØØÊèêÁ§∫‰ø°ÊÅØ
        function showRegisterErrorMsg(msg) {
            js_register_error_tip.html(msg).removeClass('vh')
        }

        // Áî®Êà∑Ê≥®ÂÜåÊ†°È™å
        function checkUserRegister() {
            var val1 = js_register_user_name.val(),
                val2 = js_register_user_phone.val(),
                val3 = js_register_user_pwd.val(),
                val6 = js_register_sms_code.val(),
                isValid = true,
                errtxt = ''
            val1 = trim(val1)
            val2 = trim(val2)
            val3 = trim(val3)
            val6 = trim(val6)
            js_register_user_name.val(val1)
            js_register_user_phone.val(val2)
            js_register_user_pwd.val(val3)
            js_register_sms_code.val(val6)
            if (val1.length === 0) {
                isValid = false
                errtxt = 'ËØ∑ËæìÂÖ•Áî®Êà∑Âêç'
            } else if (!checkUserNameLegal(val1)) {
                isValid = false
                errtxt = 'Áî®Êà∑Âêç‰∏çÂêàÊ≥ï'
            } else if (val2.length === 0) {
                isValid = false
                errtxt = 'ËØ∑ËæìÂÖ•ÊâãÊú∫Âè∑Á†Å'
            } else if (!checkPhoneNum(val2)) {
                isValid = false
                errtxt = 'ÊâãÊú∫Âè∑Á†Å‰∏çÂêàÊ≥ï'
            } else if (val3.length === 0) {
                isValid = false
                errtxt = 'ËØ∑ËæìÂÖ•ÂØÜÁ†Å'
            } else if (!checkUserPwdLegal(val3)) {
                isValid = false
                errtxt = 'ÂØÜÁ†Å‰∏çÂêàÊ≥ï'
            } else if (!barIsOk) {
                isValid = false
                errtxt = 'Êú™ÂÆåÊàêÊªëÂùóÈ™åËØÅ'
            } else if (val6.length === 0) {
                isValid = false
                errtxt = 'ËØ∑ËæìÂÖ•Áü≠‰ø°È™åËØÅÁ†Å'
            } else if (checkPureDigits(val6)) {
                isValid = false
                errtxt = 'Áü≠‰ø°È™åËØÅÁ†Å‰∏çÂêàÊ≥ï'
            } else if(!msgIsOk){
                isValid = false
                errtxt = 'ÊÇ®ËøòÊú™Ëé∑ÂèñÁü≠‰ø°È™åËØÅÁ†Å'
            }
            return { isValid, errtxt }
        }

        // Ê†°È™åÁî®Êà∑ÂêçÊòØÂê¶Â∑≤Â≠òÂú®
        function checkUserNameIsExist() {
            return new Promise(r =>
                $.ajax({
                    type: 'post',
                    url: '/user/exists',
                    data: {
                        username: js_register_user_name.val()
                    },
                    success: function (data) {
                        if (data.status == 200) {
                            r({ status: false, msg: 'ËØ•Áî®Êà∑ÂêçÂ∑≤Â≠òÂú®' })
                        } else {
                            r({ status: true, msg: 'ÂΩìÂâçË¥¶Âè∑Êú™Ë¢´Ê≥®ÂÜå' })
                        }
                    }
                })
            )
        }

        // Ê†°È™åÊâãÊú∫Âè∑ÊòØÂê¶Â∑≤Ê≥®ÂÜå
        function checkUserPhoneHasRegistered() {
            return new Promise(r =>
                $.ajax({
                    type: 'post',
                    url: '/api/mobilecheck',
                    data: {
                        mobile: js_register_user_phone.val()
                    },
                    success: function (data) {
                        if (data.status == 200) {
                            r({ status: false, msg: 'ËØ•ÊâãÊú∫Âè∑Â∑≤Ë¢´Ê≥®ÂÜå' })
                        } else {
                            r({ status: true, msg: 'ÂΩìÂâçÊâãÊú∫Âè∑Êú™Ë¢´Ê≥®ÂÜå' })
                        }
                    }
                })
            )
        }

        // Âú®ÊØè‰∏™Ê†°È™åË°®ÂçïÂÖÉÁ¥†‰∏äÁõëÂê¨‰∫ã‰ª∂
        js_register_input_txt.on({
            keyup: function (e) {
                if (e.keyCode === 13) {
                    js_btn_register.trigger('click')
                }
            },
            blur: function () {
                clearRegisterErrorTip()
            }
        })

        // Ëé∑ÂèñÈ™åËØÅÁ†ÅÂÄíËÆ°Êó∂
        function countRegisterDown(time) {
            js_register_get_sms_code.html(time + 'sÂêéÈáçÊñ∞Ëé∑Âèñ')
            time--
            if (time >= 0) {
                setTimeout(function () {
                    countRegisterDown(time)
                }, 1000)
            } else {
                js_register_get_sms_code.html('Ëé∑ÂèñÈ™åËØÅÁ†Å')
                js_register_get_sms_code.removeClass('disabled')
            }
        }

        // Ëé∑ÂèñÁü≠‰ø°È™åËØÅÁ†Å
        function getRegisterSmsCode() {
            // ajaxÊèê‰∫§Ë°®Âçï
            $.ajax({
                type: 'post',
                url: '/api/sendvalsms',
                data: {
                    mobile: js_register_user_phone.val(),
                    valtype: 1,
                    registerip: local_ip_address
                },
                success: function (data) {
                    if (data.status == 200) {
                        countRegisterDown(60)
                        msgIsOk = true
                    } else {
                        showRegisterErrorMsg(data.msg)
                        js_register_get_sms_code.removeClass('disabled')
                        
                    }
                },
                error: function () {}
            })
        }

        

        // ÁÇπÂáªËé∑ÂèñÁü≠‰ø°È™åËØÅÁ†Å
        js_register_get_sms_code.on({
            click: async function () {
                if (!$(this).hasClass('disabled')) {
                    const { errtxt } = checkUserRegister()
                    const arr = [
                        'ËØ∑ËæìÂÖ•Áî®Êà∑Âêç',
                        'Áî®Êà∑Âêç‰∏çÂêàÊ≥ï',
                        'ËØ∑ËæìÂÖ•ÊâãÊú∫Âè∑Á†Å',
                        'ÊâãÊú∫Âè∑Á†Å‰∏çÂêàÊ≥ï',
                        'ËØ∑ËæìÂÖ•ÂØÜÁ†Å',
                        'ÂØÜÁ†Å‰∏çÂêàÊ≥ï',
                        'Êú™ÂÆåÊàêÊªëÂùóÈ™åËØÅ'
                    ]
                    const res = arr.find(item => item === errtxt)
                    if (!res) {
                        const {
                            status,
                            msg
                        } = await checkUserPhoneHasRegistered()
                        if (status) {
                            $(this).addClass('disabled')
                            getRegisterSmsCode()
                        } else {
                            showRegisterErrorMsg(msg)
                            
                        }
                    } else {
                        showRegisterErrorMsg(res)
                    }
                }
            }
        })

        // ÁÇπÂáªÂàáÊç¢ÊòØÂê¶ÂêåÊÑèÁî®Êà∑Ê≥®ÂÜåÂçèËÆÆ
        js_check_protocol.on({
            click: function () {
                if ($(this).hasClass('active')) {
                    $(this).removeClass('active')
                    js_btn_register.addClass('disabled')
                } else {
                    $(this).addClass('active')
                    js_btn_register.removeClass('disabled')
                }
            }
        })

        // ÁÇπÂáªÊ≥®ÂÜå
        js_btn_register.on({
            click: async function (e) {
                e.stopPropagation()
                if (!$(this).hasClass('disabled')) {
                    const { isValid, errtxt } = checkUserRegister()
                    if (isValid) {
                        const { status, msg } = await checkUserNameIsExist()
                        if (status) {
                            $.ajax({
                                type: 'post',
                                url: '/api/register',
                                data: {
                                    name: js_register_user_name.val(),
                                    mobile: js_register_user_phone.val(),
                                    password: js_register_user_pwd.val(),
                                    smscode: js_register_sms_code.val()
                                },
                                success: function (data) {
                                    // that.canCommit = false
                                    if (data.status == 200) {
                                        alert('Ê≥®ÂÜåÊàêÂäü')
                                        $.ajax({
                                            type: 'post',
                                            url: '/api/login',
                                            data: {
                                                name: js_register_user_name.val(),
                                                password: js_register_user_pwd.val()
                                            },
                                            success(res) {
                                                if (res.status == 200) {
                                                    alert('Ëá™Âä®ÁôªÂΩïÊàêÂäü')
                                                    window.location.reload()
                                                } else {
                                                    alert(res.msg)
                                                }
                                            }
                                        })
                                    } else {
                                        showRegisterErrorMsg(data.msg)
                                    }
                                }
                                // ,
                                // error: function () {
                                //     that.canCommit = false
                                // }
                            })
                        } else {
                            showRegisterErrorMsg(msg)
                        }
                    } else {
                        showRegisterErrorMsg(errtxt)
                    }
                }
            }
        })
    }



















    
    /**
     * ‰∏éÂøòËÆ∞ÂØÜÁ†ÅÁõ∏ÂÖ≥ÁöÑÊìç‰Ωú
     */

    // Ê∏ÖÈô§ÂøòËÆ∞ÂØÜÁ†ÅÈîôËØØÊèêÁ§∫
    function clearForgotErrorTip() {
        js_forgot_error_tip.addClass('vh')
    }

    //Âà∑Êñ∞ÂõæÁâáÈ™åËØÅÁ†Å
    function refreshForgotCaptchaCode() {
        var src = '/img/captcha.jpg?width=86&height=44&t=' + +new Date()
        js_forgot_captcha_code.attr('src', src)
    }

    //ÁÇπÂáªÂà∑Êñ∞ÂõæÁâáÈ™åËØÅÁ†Å
    js_forgot_captcha_code.on({
        click: refreshForgotCaptchaCode
    })

    // ÊòæÁ§∫ÂøòËÆ∞ÂØÜÁ†ÅÈîôËØØÊèêÁ§∫‰ø°ÊÅØ
    function showForgotErrorMsg(msg) {
        js_forgot_error_tip.html(msg).removeClass('vh')
    }

    // Áî®Êà∑ÂøòËÆ∞ÂØÜÁ†ÅÊ†°È™å
    function checkUserForgot() {
        var val1 = js_forgot_user_phone.val(),
            val2 = js_forgot_img_code.val(),
            val3 = js_forgot_sms_code.val(),
            val4 = js_forgot_user_pwd.val(),
            val5 = js_forgot_repeat_pwd.val(),
            isValid = true
        val1 = trim(val1)
        val2 = trim(val2)
        val3 = trim(val3)
        val4 = trim(val4)
        val5 = trim(val5)
        js_forgot_user_phone.val(val1)
        js_forgot_img_code.val(val2)
        js_forgot_sms_code.val(val3)
        js_forgot_user_pwd.val(val4)
        js_forgot_repeat_pwd.val(val5)
        if (val1.length === 0) {
            showForgotErrorMsg('ËØ∑ËæìÂÖ•ÊâãÊú∫Âè∑Á†Å')
            isValid = false
        } else if (!checkPhoneNum(val1)) {
            showForgotErrorMsg('ÊâãÊú∫Âè∑Á†Å‰∏çÂêàÊ≥ï')
            isValid = false
        } else if (val2.length === 0) {
            showForgotErrorMsg('ËØ∑ËæìÂÖ•ÂõæÁâáÈ™åËØÅÁ†Å')
            isValid = false
        } else if (val3.length === 0) {
            showForgotErrorMsg('ËØ∑ËæìÂÖ•Áü≠‰ø°È™åËØÅÁ†Å')
            isValid = false
        } else if (checkPureDigits(val3)) {
            showForgotErrorMsg('Áü≠‰ø°È™åËØÅÁ†Å‰∏çÂêàÊ≥ï')
            isValid = false
        } else if (val4.length === 0) {
            showForgotErrorMsg('ËØ∑ËæìÂÖ•ÂØÜÁ†Å')
            isValid = false
        } else if (!checkUserPwdLegal(val4)) {
            showForgotErrorMsg('ÂØÜÁ†Å‰∏çÂêàÊ≥ï')
            isValid = false
        } else if (val5.length === 0) {
            showForgotErrorMsg('ËØ∑Á°ÆËÆ§ÂØÜÁ†Å')
            isValid = false
        } else if (val4 !== val5) {
            showForgotErrorMsg('‰∏§Ê¨°ËæìÂÖ•ÁöÑÂØÜÁ†Å‰∏ç‰∏ÄËá¥')
            isValid = false
        }
        if (!isValid) {
            refreshForgotCaptchaCode()
        }
        return isValid
    }

    // Ê†°È™åÊâãÊú∫Âè∑ÊòØÂê¶Â∑≤Ê≥®ÂÜå
    function checkUserPhoneHasForgot(phoneNum) {
        if (checkPhoneNum(phoneNum)) {
            $.ajax({
                type: 'post',
                url: '/api/mobilecheck',
                data: {
                    mobile: js_forgot_user_phone.val()
                },
                success: function (data) {
                    if (data.status !== 200) {
                        showForgotErrorMsg('ËØ•ÊâãÊú∫Âè∑‰∏çÂ≠òÂú®')
                        forgot_user_phone_is_exist = false
                    } else {
                        clearForgotErrorTip()
                        forgot_user_phone_is_exist = true
                    }
                }
            })
        }
    }

    // Âú®ÊØè‰∏™Ê†°È™åË°®ÂçïÂÖÉÁ¥†‰∏äÁõëÂê¨‰∫ã‰ª∂
    js_forgot_input_txt.on({
        keyup: function (e) {
            if (e.keyCode === 13) {
                js_btn_forgot.trigger('click')
            }
        },
        blur: function () {
            clearForgotErrorTip()
        }
    })

    // Èô§‰∫ÜÊâãÊú∫Âè∑Á†ÅËæìÂÖ•Ê°Ü‰ª•Â§ñÁöÑËæìÂÖ•Ê°ÜÁªëÂÆö‰∫ã‰ª∂
    js_forgot_input_txt.not(js_forgot_user_phone).on({
        focus: function () {
            if (forgot_user_phone_is_blur) {
                var phoneNum = js_forgot_user_phone.val()
                forgot_user_phone_is_blur = false
                checkUserPhoneHasForgot(phoneNum)
            }
        }
    })

    // Â§±ÂéªÁÑ¶ÁÇπÊó∂Ê†°È™åÊâãÊú∫Âè∑Á†ÅÊòØÂê¶Â∑≤Ê≥®ÂÜå
    js_forgot_user_phone.on({
        blur: function () {
            forgot_user_phone_is_blur = true
        }
    })

    // Ëé∑ÂèñÈ™åËØÅÁ†ÅÂÄíËÆ°Êó∂
    function countForgotDown(time) {
        js_forgot_get_sms_code.html(time + 'sÂêéÈáçÊñ∞Ëé∑Âèñ')
        time--
        if (time >= 0) {
            setTimeout(function () {
                countForgotDown(time)
            }, 1000)
        } else {
            js_forgot_get_sms_code.html('Ëé∑ÂèñÈ™åËØÅÁ†Å')
            js_forgot_get_sms_code.removeClass('disabled')
        }
    }

    // ÂøòËÆ∞ÂØÜÁ†Å-Ëé∑ÂèñÁü≠‰ø°È™åËØÅÁ†Å
    function getForgotSmsCode() {
        // ajaxÊèê‰∫§Ë°®Âçï
        $.ajax({
            type: 'post',
            url: '/api/sendvalsms',
            data: {
                mobile: js_forgot_user_phone.val(),
                ticket: js_forgot_img_code.val(),
                valtype: 1
            },
            success: function (data) {
                if (data.status == 200) {
                    countForgotDown(60)
                } else {
                    refreshForgotCaptchaCode()
                    showForgotErrorMsg(data.msg)
                    js_forgot_get_sms_code.removeClass('disabled')
                }
            },
            error: function () {}
        })
    }

    // ÂøòËÆ∞ÂØÜÁ†Å-ÊòØÂê¶ÂèØ‰ª•ÂèëÈÄÅÁü≠‰ø°È™åËØÅÁ†Å
    function canSendForgotSmsCode() {
        var val1 = js_forgot_user_phone.val(),
            val2 = js_forgot_img_code.val(),
            isValid = true
        val1 = trim(val1)
        val2 = trim(val2)
        js_forgot_user_phone.val(val1)
        js_forgot_img_code.val(val2)
        if (val1.length === 0) {
            showForgotErrorMsg('ËØ∑ËæìÂÖ•ÊâãÊú∫Âè∑Á†Å')
            isValid = false
        } else if (!checkPhoneNum(val1)) {
            showForgotErrorMsg('ÊâãÊú∫Âè∑Á†Å‰∏çÂêàÊ≥ï')
            isValid = false
        } else if (val2.length === 0) {
            showForgotErrorMsg('ËØ∑ËæìÂÖ•ÂõæÁâáÈ™åËØÅÁ†Å')
            isValid = false
        }
        if (!isValid) {
            refreshForgotCaptchaCode()
        }
        return isValid
    }

    // ÂøòËÆ∞ÂØÜÁ†Å-ÁÇπÂáªËé∑ÂèñÁü≠‰ø°È™åËØÅÁ†Å
    js_forgot_get_sms_code.on({
        click: function () {
            if (!$(this).hasClass('disabled') && canSendForgotSmsCode()) {
                if (forgot_user_phone_is_exist) {
                    $(this).addClass('disabled')
                    clearForgotErrorTip()
                    getForgotSmsCode()
                } else {
                    showForgotErrorMsg('ËØ•ÊâãÊú∫Âè∑‰∏çÂ≠òÂú®')
                    refreshForgotCaptchaCode()
                }
            }
        }
    })

    // ÁÇπÂáªÂøòËÆ∞ÂØÜÁ†Å
    js_btn_forgot.on({
        click: function (e) {
            e.stopPropagation()
            if (checkUserForgot() && !this.canCommit) {
                var that = this
                this.canCommit = true
                // ajaxÊèê‰∫§Ë°®Âçï
                $.ajax({
                    type: 'post',
                    url: '/api/resetpwd',
                    data: {
                        mobile: js_forgot_user_phone.val(),
                        password: js_forgot_user_pwd.val(),
                        mobilecode: js_forgot_sms_code.val()
                    },
                    success: function (data) {
                        that.canCommit = false
                        if (data.status == 200) {
                            alert('ÈáçÁΩÆÂØÜÁ†ÅÊàêÂäüÔºåËØ∑ÈáçÊñ∞ÁôªÂΩï')
                            openPopBox(js_pop_user, 0)
                        } else {
                            refreshForgotCaptchaCode()
                            showForgotErrorMsg(data.msg)
                        }
                    },
                    error: function () {
                        that.canCommit = false
                    }
                })
            }
        }
    })
})(jQuery)
