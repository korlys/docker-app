;(function ($) {
    // ‰ª•‰∏ãÊòØÂâçÁ´ØÊâÄÈúÄË¶ÅÁöÑÊâÄÊúâÂèòÈáè
    // ÂÖ¨ÂÖ±ÂèòÈáè
